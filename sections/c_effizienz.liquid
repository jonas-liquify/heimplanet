                    <div li-block="Kachel" id="w-node-_41e33fc8-610d-205a-b3da-086662393623-62393622" class="points_grid-item">
                                    <img loading="lazy" alt="{{ block.settings.image_icon.alt }}" src="{{ block.settings.image_icon | img_url: 'master' }}" class="icon-1x1-medium"}>

