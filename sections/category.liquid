<div class="background-color-light-grey" style="background-color: {{ section.settings.background }}" >

<div class="padding-global padding-section-large"><div class="container-large"><div class="product_component"><div class="product_content-top"><div class="subline op_70">
{{ section.settings.text_subheading }}
</div>
<h2 class="heading-style-h2">
{{ section.settings.text_main_heading }}
</h2>
</div><div class="swiper is-products w-dyn-list"><div role="list" class="swiper-wrapper is-products w-dyn-items">{% for product in section.settings.collection_collection.products limit: section.settings.collection_collection_limit %}
<div class="swiper-slide is-products w-dyn-item">
    
    {% render 'product_item', product: product %}

    
</div>
{% endfor %}
</div><div class="hide w-dyn-empty"><div>No items found.</div></div></div><div class="swiper_arrow-holder"><div class="swiper_arrow-holder-inner"><div product-prev="" class="icon-embed-small is-prev w-embed"><svg xmlns="http://www.w3.org/2000/svg" width="100%" height="100%" viewbox="0 0 32 32" fill="none" preserveaspectratio="xMidYMid meet" aria-hidden="true" role="img"><mask id="mask0_2623_5545" style="mask-type:alpha" maskunits="userSpaceOnUse" x="0" y="0" width="32" height="32"><rect width="32" height="32" fill="#D9D9D9"></rect></mask><g mask="url(#mask0_2623_5545)"><path d="M21.5665 17.3333H5.33313V14.6666H21.5665L14.0998 7.19992L15.9998 5.33325L26.6665 15.9999L15.9998 26.6666L14.0998 24.7999L21.5665 17.3333Z" fill="#050400"></path></g></svg></div><div product-next="" class="icon-embed-small is-next w-embed"><svg xmlns="http://www.w3.org/2000/svg" width="100%" height="100%" viewbox="0 0 32 32" fill="none" preserveaspectratio="xMidYMid meet" aria-hidden="true" role="img"><mask id="mask0_2623_5545" style="mask-type:alpha" maskunits="userSpaceOnUse" x="0" y="0" width="32" height="32"><rect width="32" height="32" fill="#D9D9D9"></rect></mask><g mask="url(#mask0_2623_5545)"><path d="M21.5665 17.3333H5.33313V14.6666H21.5665L14.0998 7.19992L15.9998 5.33325L26.6665 15.9999L15.9998 26.6666L14.0998 24.7999L21.5665 17.3333Z" fill="#050400"></path></g></svg></div></div><a class="button hide-mobile-portrait w-button" href="{{ section.settings.url_button_url }}"> {{ section.settings.text_button_text }} </a>
</div><div class="hide w-embed w-script"><script>document.addEventListener("DOMContentLoaded", function() {
            var swiper = new Swiper(".is-products", {
                slidesPerView: 4,
                spaceBetween: 12,
                effect: 'slide',
                speed: 800,
                a11y: false,
                navigation: {
                    nextEl: "[product-next]",
                    prevEl: "[product-prev]",
                },
                breakpoints: {
                    // when window width is >= 320px
                    320: {
                        slidesPerView: 1,
                        spaceBetween: 12
                    },
                    // when window width is >= 640px
                    640: {
                        slidesPerView: 2,
                        spaceBetween: 12
                    },
                    // when window width is >= 1024px
                    1024: {
                        slidesPerView: 4,
                        spaceBetween: 12
                    }
                }
            });
        });
    </script></div></div></div></div><div class="hide">
{{ section.settings.text_background }}
</div>
</div>
 {% schema %} {"tag":"section","name":"category","class":"section_product","settings":[{"id":"text_subheading","type":"text","label":"Subheading","default":"Mining Hardware von marktf\u00fchrenden Herstellern"},{"id":"text_main_heading","type":"text","label":"Main Heading","default":"Entdecken Sie unsere Bitcoin Miner"},{"id":"collection_collection","type":"collection","label":"Collection"},{"type":"range","id":"collection_collection_limit","min":1,"max":99,"step":1,"label":"Limit: Collection","default":6},{"id":"url_button_url","type":"url","label":"Button-Url"},{"id":"text_button_text","type":"text","label":"Button-Text","default":"Alle Produkte anzeigen"},{"id":"text_background","type":"text","label":"Background","default":"This is some text inside of a div block."}],"presets":[{"name":"category","category":"Liquify"}]} {% endschema %}