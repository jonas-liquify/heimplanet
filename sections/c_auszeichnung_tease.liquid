                    <div li-block="Auszeichnung Item" id="w-node-c2d61e16-b16b-0278-66a8-d8e131c4c2d0-31c4c2cb" class="teaser_auszeichnung_item">
                                    <img loading="lazy" alt="{{ block.settings.image_bild.alt }}" src="{{ block.settings.image_bild | img_url: 'master' }}" class="teaser_auszeichnung_image"}>

                                    <div class="teaser_auszeichnng_content"><div class="text-size-medium text-weight-medium"> {{ block.settings.richtext_paragraph }} </div>
<a href="{{ block.settings.url_button_link }}" class="button is-secondary is-icon w-inline-block"> <img loading="lazy" src="{{ 'arrow-side-top.svg' | asset_url }}" alt="" class="icon-1x1-semi-medium"><div>{{ block.settings.text_button_text }}</div> </a>
</div>
                            </div>
            {% endif %}
{% endfor %}
</div></div></div>{% if section.settings.checkbox_pfeil == true %}
