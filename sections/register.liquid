<div class="padding-global"><div class="container-large"><div class="login_content"><div class="login_content-inner"><div class="login_form w-form"><h1 class="heading-style-h5 text-align-center">Registration</h1>{% form 'create_customer', return_to: '/', class: 'form_form' %}
<div class="form_field-wrapper-element"><div class="form_field-wrapper"><div class="input_tag">First name</div><input class="form-input-standard w-input" maxlength="256" name="customer[first_name]" data-name="Field 5" placeholder="Enter your first name" type="text" id="field-5">
</div><div class="form_field-wrapper"><div class="input_tag">Last name</div><input class="form-input-standard w-input" maxlength="256" name="customer[last_name]" data-name="Field 5" placeholder="Enter your last name" type="text" id="field-5">
</div><div class="form_field-wrapper"><div class="input_tag">E-MAIL</div><input class="form-input-standard w-input" maxlength="256" name="customer[email]" data-name="Field 5" placeholder="Enter your e-mail adress" type="email" id="field-5">
</div><div class="form_field-wrapper"><div class="input_tag">Password</div><input class="form-input-standard w-input" maxlength="256" name="customer[password]" data-name="Field 5" placeholder="Enter your  password" type="password" id="field-5" required="">
</div></div><input type="submit" data-wait="Please Wait" class="checkout-button w-button" value="Register">
{% if form.errors %}    <div li-if="form.errors">
    <div li-object="form.errors | default_errors" class="text-size-regular text-color-red text-align-center">{{ form.errors | default_errors }}</div>    </div>
{% endif %}{% endform %}
<div class="text-size-regular text-align-center"><span class="text-color-hazel90 text-weight-semibold">Already having  an account? </span><a href="{{ routes.account_login_url }}" li-object:href="routes.account_login_url"><span li-object:href="routes.account_login_url" class="text-style-link text-weight-semibold" href="{{ routes.account_login_url }}">Login in</span></a></div><div class="w-form-done"><div>Thank you! Your submission has been received!</div></div><div class="login_hinweis-kachel w-form-fail"><div>Oops! Something went wrong while submitting the form.</div></div></div></div></div></div></div> {% schema %} {"tag":"section","name":"register","class":"section_login background-color-standard-grey","presets":[{"name":"register","category":"Liquify"}]} {% endschema %}