<script src="{{ 'liquify_custom.js' | asset_url }}" type="text/javascript"></script>
<div li-element="product-variant-container" class="product-item_component" x-data="handleVariant({{ product | json | escape }}, {{ product.options_by_name | json | escape }}, {{ product.selected_or_first_available_variant | json | escape }})" x-init="$nextTick(() => { activateVariants() })" data-productid="{{ product.id }}"><div class="product-item_image-link"><a li-object:href="product.url" href="{{ product.url }}" class="product-item_image-wrapper w-inline-block"><img li-object:alt="product.featured_image.alt" loading="lazy" li-object:src="product.featured_image | img_url: 'large'" src="{{ product.featured_image | img_url: 'large' }}" alt="{{ product.featured_image.alt }}" class="product-item_image is-first">
<img li-object:alt="product.images[1].alt" loading="lazy" li-object:src="product.images[1] | img_url: 'large'" src="{{ product.images[1] | img_url: 'large' }}" alt="{{ product.images[1].alt }}" class="product-item_image is-second">
</a><div class="product-item_tag_list">{% if product.compare_at_price_max > product.price %}    <div li-if="product.compare_at_price_max > product.price" class="product-item_tag hide">
    Special    </div>
{% endif %}{% if product.selected_or_first_available_variant.available != true %}    <div li-if="product.selected_or_first_available_variant.available != true" class="product-item_tag">
    Sold Out    </div>
{% endif %}{% for tag in product.metafields.custom.produkt_tags.value %}    <div li-for="tag in product.metafields.custom.produkt_tags.value" class="product-item_tag">
                                    <span li-object="tag">{{ tag }}</span>
                        </div>
{% endfor %}</div></div><div class="spacer-xxsmall"></div><a li-object:href="product.url" href="{{ product.url }}" class="product-item_text-link w-inline-block"><div class="heading-style-h3 is-product-title"><span li-object="product.title">{{ product.title }}</span></div><div class="product-item_price-wrapper"><div li-object="product.type" class="text-size-small text-color-grey">{{ product.type }}</div><div class="product-item_price">{% if product.compare_at_price_max > product.price %}    <div li-if="product.compare_at_price_max > product.price">
    <div li-object="product.compare_at_price | money" class="text-size-regular text-style-strikethrough text-style-muted is-price">{{ product.compare_at_price | money }}</div>    </div>
{% endif %}<div li-object="product.price | money" class="text-size-regular is-price">{{ product.price | money }}</div></div></div></a></div>
